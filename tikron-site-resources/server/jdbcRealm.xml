<?xml version="1.0" encoding="UTF-8"?>

<!-- Security realm for Tikron -->
<!-- Copy this to the "Host" section of Tomcat server.xml -->
<Realm className="org.apache.catalina.realm.DataSourceRealm"
	dataSourceName="jdbc/tikron"
	userTable="common_user"
	userNameCol="name"
	userCredCol="password"
	userRoleTable="common_user"
	roleNameCol="role" />
