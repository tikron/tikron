<?xml version="1.0" encoding="UTF-8"?>
<it:templates xmlns:it="http://www.tikron.de/imageTemplates" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.tikron.de/imageTemplates ../../../ibase/ibase/target/classes/imageTemplates.xsd ">
	<!-- *** Tikron Webapp -->
	<template>
		<name>galImgLg</name>
		<creationType>EAGER</creationType>
		<scaling>
			<mode>LIMITTED</mode>
			<height>560</height>
		</scaling>
		<signature>
			<text>tikron.de</text>
			<size>14</size>
			<opacity>0.4</opacity>
			<position>
				<horizontal>10</horizontal>
				<vertical>-10</vertical>
			</position>
		</signature>
		<compressionQuality>1.0</compressionQuality>
	</template>
	<template>
		<name>galImgMd</name>
		<creationType>EAGER</creationType>
		<scaling>
			<mode>LIMITTED</mode>
			<height>480</height>
		</scaling>
		<signature>
			<text>tikron.de</text>
			<size>12</size>
			<opacity>0.4</opacity>
			<position>
				<horizontal>10</horizontal>
				<vertical>-10</vertical>
			</position>
		</signature>
		<compressionQuality>1.0</compressionQuality>
	</template>
	<template>
		<name>galImgSm</name>
		<creationType>EAGER</creationType>
		<scaling>
			<mode>LIMITTED</mode>
			<height>320</height>
		</scaling>
		<signature>
			<text>tikron.de</text>
			<size>12</size>
			<opacity>0.4</opacity>
			<position>
				<horizontal>10</horizontal>
				<vertical>-10</vertical>
			</position>
		</signature>
		<compressionQuality>1.0</compressionQuality>
	</template>
	<template>
		<name>galImgXs</name>
		<creationType>EAGER</creationType>
		<scaling>
			<mode>LIMITTED</mode>
			<height>200</height>
		</scaling>
		<signature>
			<text>tikron.de</text>
			<size>10</size>
			<opacity>0.4</opacity>
			<position>
				<horizontal>10</horizontal>
				<vertical>-10</vertical>
			</position>
		</signature>
		<compressionQuality>1.0</compressionQuality>
	</template>
	<template>
		<name>galleryThumbnail</name>
		<creationType>EAGER</creationType>
		<scaling>
			<mode>ABSOLUTE</mode>
			<height>66</height>
		</scaling>
	</template>
	<template>
		<name>galleryTeaser</name>
		<scaling>
			<mode>ABSOLUTE</mode>
			<height>150</height>
		</scaling>
	</template>
	<template>
		<name>galleryStory</name>
		<scaling>
			<mode>ABSOLUTE</mode>
			<height>200</height>
		</scaling>
	</template>
	<!-- *** Tikron Manager -->
	<template>
		<name>galleryList</name>
		<scaling>
			<mode>ABSOLUTE</mode>
			<width>40</width>
			<height>40</height>
		</scaling>
	</template>
	<template>
		<name>galleryEdit</name>
		<scaling>
			<mode>LIMITTED</mode>
			<width>150</width>
			<height>150</height>
		</scaling>
	</template>
</it:templates>
